{
    "name": "dustin-affinityps/laravel-ab",
    "description": "Laravel A/B experiment testing tool",
    "keywords": [
        "ab-testing"
    ],
    "homepage": "https://github.com/dustin-affinityps/laravel-ab",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ben",
            "email": "moin@benjaminbortels.de",
            "role": "Developer"
        }, {
            "name": "Dustin",
            "email": "dustin@affinityps.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.6.* | 5.7.* | 5.8.* | ^6.0",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.* | 3.7.* | 3.8.*",
        "phpunit/phpunit": "^7.0 | ^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "dustin-affinityps\\AbTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dustin-affinityps\\AbTesting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "dustin-affinityps\\AbTesting\\AbTestingServiceProvider"
            ],
            "aliases": {
                "AbTesting": "dustin-affinityps\\AbTesting\\AbTestingFacade"
            }
        }
    }
}
